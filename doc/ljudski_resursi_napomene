ALTER TABLE  `osoba` ADD  `djevojacko_prezime` VARCHAR( 50 ) NOT NULL;
ALTER TABLE  `osoba` ADD  `maternji_jezik` INT NOT NULL;
ALTER TABLE  `osoba` ADD  `vozacka_dozvola` INT NOT NULL;
ALTER TABLE  `osoba` ADD  `mobilni_telefon` VARCHAR( 15 ) NOT NULL;

CREATE TABLE IF NOT EXISTS `vozacka_dozvola` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `fk_osoba` int(11) NOT NULL,
  `fk_vozacki_kategorija` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COLLATE=utf8_slovenian_ci AUTO_INCREMENT=1 ;


